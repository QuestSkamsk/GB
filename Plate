class Plate {

    private int food;

    int getFood() {
        return food;
    }

    Plate(int food) {
        this.food = food;
    }

    void decreaseFood(int n) {
        food -= n;
    }

    void increaseEat() {
        this.food += 400;
        System.out.println("Добавили ещё 400 грамм корма");
    }

    boolean checkEat(int n) {
        return (food - n) >= 0;
    }

}